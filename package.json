{
  "name": "vue3-project",
  "version": "0.0.0",
  "scripts": {
    "start:dev": "vite --mode development",
    "start:test": "vite --mode test",
    "start:prod": "vite --mode prod",
    "build:dev": "run-p type-check build-dev-only",
    "build:test": "run-p type-check build-test-only",
    "build:prod": "run-p type-check build-prod-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root  src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-dev-only": "vite build  --mode development",
    "build-test-only": "vite build  --mode test",
    "build-prod-only": "vite build  --mode prod",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "axios": "^1.1.3",
    "element-plus": "^2.2.26",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pinia": "^2.0.23",
    "pinia-persistedstate-plugin": "^0.1.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "cypress": "^10.10.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.2",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  },
  "lint-staged": {
    "src/**/*.{js,vue,ts,tsx, jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
    ]
  }
}
